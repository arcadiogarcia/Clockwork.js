<?xml version="1.0" encoding="utf-8" ?>
<spritesheets>
  <spritesheet name="background" src="../shared/background.png">

    <states>
      <state name="moving">
        <layer name="moving"></layer>
      </state>
      <state name="idle">
        <layer name="idle"></layer>
      </state>
    </states>

    <layers>
      <layer name="moving" x="-(t/10%800)" y="0">
        <frame name="idle"></frame>
      </layer>
      <layer name="idle" x="0" y="0">
        <frame name="idle"></frame>
      </layer>
    </layers>

    <frames>
      <frame name="idle" x="0" y="0" w="1600" h="400" t="0"></frame>

    </frames>

  </spritesheet>
  
  <spritesheet name="pipe" src="../shared/pipe.png">

    <states>
      <state name="normal">
        <layer name="Idle1"></layer>
      </state>
      <state name="reverse" flip="v">
        <layer name="Idle1"></layer>
      </state>
    </states>

    <layers>
      <layer name="Idle1" x="0" y="0">
        <frame name="I1"></frame>
      </layer>
    </layers>

    <frames>
      <frame name="I1" x="0" y="0" w="100" h="300" t="0"></frame>
    
    </frames>

  </spritesheet>

  <spritesheet name="button" src="../shared/button.png">

    <states>
      <state name="normal">
        <layer name="Idle1"></layer>
      </state>
    </states>

    <layers>
      <layer name="Idle1" x="0" y="0">
        <frame name="I1"></frame>
      </layer>
    </layers>

    <frames>
      <frame name="I1" x="0" y="0" w="200" h="100" t="0"></frame>

    </frames>

  </spritesheet>

  <spritesheet name="paradog" src="../shared/paradog_small.png">

    <states>
      <state name="IdleL">
        <layer name="Idle1"></layer>
      </state>
      <state name="IdleR" flip="h">
        <layer name="Idle1"></layer>
      </state>
      <state name="RunL">
        <layer name="Run1"></layer>
      </state>
      <state name="RunR" flip="h">
        <layer name="Run1"></layer>
      </state>
      <state name="SeeUL">
        <layer name="SeeU1"></layer>
      </state>
      <state name="SeeUR" flip="h">
        <layer name="SeeU1"></layer>
      </state>
      <state name="BarkL">
        <layer name="Bark1"></layer>
      </state>
      <state name="BarkR" flip="h">
        <layer name="Bark1"></layer>
      </state>
      <state name="ScareL">
        <layer name="Scare1"></layer>
      </state>
      <state name="ScareR" flip="h">
        <layer name="Scare1"></layer>
      </state>
    </states>

    <layers>
      <layer name="Idle1" x="0" y="0">
        <frame name="I1"></frame>
        <frame name="I2"></frame>
        <frame name="I3"></frame>
        <frame name="I4"></frame>
        <frame name="I5"></frame>
        <frame name="I6"></frame>
      </layer>
      <layer name="Run1" x="0" y="0">
